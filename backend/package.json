{
  "name": "k12-cms-backend",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "migrate:make": "knex migrate:make",
    "migrate:subscriptions": "knex migrate:latest --knexfile ./knexfile.js --knexfile ./knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.js",
    "seed": "knex seed:run --knexfile ./knexfile.js",
    "export:static": "node src/export-static.js",
    "import:subscriptions": "node src/services/importSubscriptions.js",
    "test:smoke": "bash ../scripts/smoke-test.sh",
    "test:themes": "bash ../scripts/test-themes.sh"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "multer": "^1.4.5-lts.1",
    "objection": "^3.0.1",
    "sharp": "^0.33.5",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=16.18.1"
  }
}
